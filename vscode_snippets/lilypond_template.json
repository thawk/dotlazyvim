{
    "skel": {
        "prefix": "skel",
        "body": [
            "%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%",
            "% 复调模板",
            "%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%",
            "\\version \"2.23.2\"",
            "",
            "showTN = { \\override TupletNumber #'transparent = ##f }",
            "hideTN = { \\override TupletNumber #'transparent = ##t }",
            "",
            "staffDown = { \\change Staff = \"down\" } %staff down",
            "staffUp   = { \\change Staff = \"up\"   } %staff up",
            "",
            "mbreak = \\break",
            "",
            "%%%%%%%%%%%%%%",
            "%     up     %",
            "%%%%%%%%%%%%%%",
            "A = \\relative c'",
            "{",
            "\t\\clef treble",
            "\t\\key c \\major",
            "\t\\time 4/4",
            "\t\\override Score.MetronomeMark #'transparent = ##t",
            "\t\\tempo 4=132",
            "",
            "}",
            "",
            "B = \\relative c'",
            "{",
            "",
            "}",
            "",
            "%%%%%%%%%%%%%%",
            "%    down    %",
            "%%%%%%%%%%%%%%",
            "C = \\relative c",
            "{",
            "\t\\clef bass",
            "\t\\key c \\major",
            "",
            "}",
            "",
            "D = \\relative c",
            "{",
            "",
            "}",
            "",
            "\\score",
            "{",
            "\t\\context PianoStaff",
            "\t<<",
            "\t\t%\\set PianoStaff.connectArpeggios = ##t",
            "\t\t%\\set PianoStaff.instrumentName = \\markup { \\fontsize #+3 \\bold \"1.\" }",
            "\t\t\\new Staff = \"up\"",
            "\t\t<<",
            "\t\t\\new Voice = \"A\" \\A",
            "\t\t\\new Voice = \"B\" \\B",
            "\t\t>>",
            "\t\t\\new Staff = \"down\"",
            "\t\t<<",
            "\t\t\\new Voice = \"C\" \\C",
            "\t\t\\new Voice = \"D\" \\D",
            "\t\t>>",
            "\t>>",
            "",
            "\t\\layout { }",
            "",
            "\t\\midi",
            "\t{",
            "\t\t\\context",
            "\t\t{",
            "\t\t\t\\Voice",
            "\t\t\t\\remove Dynamic_performer",
            "\t\t}",
            "\t}",
            "}"
        ],
        "description": "复调钢琴作品模板"
    }
}